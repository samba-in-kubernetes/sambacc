{
  "samba-container-config": "v0",
  "configs": {
    "demo": {
      "shares": [
        "share"
      ],
      "globals": [
        "default"
      ],
      "instance_name": "SAMBA"
    }
  },
  "shares": {
    "share": {
      "options": {
        "path": "/share",
        "valid users": "sambauser, otheruser"
      }
    }
  },
  "globals": {
    "default": {
      "options": {
        "security": "user",
        "server min protocol": "SMB2",
        "load printers": "no",
        "printing": "bsd",
        "printcap name": "/dev/null",
        "disable spoolss": "yes",
        "guest ok": "no"
      }
    }
  },
  "users": {
    "all_entries": [
      {
        "name": "sambauser",
        "password": "samba"
      },
      {
        "name": "otheruser",
        "password": "insecure321"
      }
    ]
  },
  "_footer": 1
}
